{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "off",
    "no-unused-vars": "off",
    "prefer-const": "off",
    "react-hooks/exhaustive-deps": "off",
    "react/jsx-key": "off",
    "react/no-unescaped-entities": "off",
    "no-console": "off",
    "no-var": "off"
  }
}
